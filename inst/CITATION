year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "CMET FFQ",
         author = c(person("Yorick", "Minnebo")),
         year = year,
         note = note,
         url = "https://github.ugent.be/yminnebo/CMET_FFQ")
